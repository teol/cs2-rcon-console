    "@types/jsdom": "^28",
    "@types/node": "20",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
