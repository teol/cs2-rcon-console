{
  "name": "cs2-web-rcon",
  "version": "2.0.0",
  "description": "Web-based RCON console for Counter-Strike 2 servers",
  "type": "module",
  "main": "dist/server.js",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc --watch & node --watch dist/server.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@fastify/static": "^8.1.0",
    "@fastify/websocket": "^11.0.2",
    "fastify": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "prettier": "^3.8.1",
    "typescript": "^5.8.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
